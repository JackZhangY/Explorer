{
  "env": [
    {
      "name": ["CartPole-v1"],
      "max_episode_steps": [-1],
      "input_type": ["feature"]
    }
  ],
  "agent": [
    {"name": ["VanillaDQN", "DQN", "DDQN"]},
    {
      "name": ["MaxminDQN"],
      "target_networks_num": [2]
    }
  ],
  "train_steps": [5e5],
  "test_per_episodes": [50],
  "device": ["cpu"],
  "hidden_layers": [[64,64]],
  "memory_size": [1e4],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [1e3],
  "epsilon_steps": [1e3],
  "epsilon_start": [1.0],
  "epsilon_end": [0.01],
  "epsilon_decay": [0.999],
  "loss": ["MSELoss"],
  "optimizer": [
    {
      "name": ["RMSprop"],
      "kwargs": [{"lr": [1e-4]}]
    }
  ],
  "batch_size": [32],
  "display_interval": [100],
  "rolling_score_window": [{"Train": [20], "Test": [10]}],
  "discount": [0.99],
  "seed": [1],
  "show_tb": [true],
  "gradient_clip": [5],
  "target_network_update_frequency": [200],
  "network_update_frequency": [1],
  "generate_random_seed": [true]
}