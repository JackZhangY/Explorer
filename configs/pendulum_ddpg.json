{
  "env": [
    {
      "name": ["Pendulum-v0"],
      "max_episode_steps": [-1],
      "input_type": ["feature"]
    }
  ],
  "train_steps": [5e4], 
  "test_per_episodes": [10],
  "exploration_steps": [1e3],
  "memory_size": [1e4],
  "agent": [{"name": ["DDPG"]}],
  "optimizer": [
    {
      "name": ["Adam"],
      "actor_kwargs": [{"lr": [3e-4]}],
      "critic_kwargs": [{"lr": [1e-3]}]
    }
  ],
  "batch_size": [32],
  "network_update_frequency": [1],
  "discount": [0.99],
  "polyak": [0.995],
  "action_noise": [0.1],
  "seed": [1],
  "gradient_clip": [-1],
  "hidden_layers": [[64,64]],
  "hidden_activation": ["Tanh"],
  "display_interval": [10],
  "rolling_score_window": [{"Train": [50], "Test": [10]}],
  "device": ["cpu"],
  "show_tb": [true],
  "generate_random_seed": [true]
}