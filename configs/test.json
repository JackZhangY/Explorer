{
  "env": [
    {
      "name": ["Pixelcopter-v0"],
      "max_episode_steps": [500],
      "train_steps": [4e5],
      "test_per_episodes": [10],
      "input_type": ["feature"]
    }
  ],
  "agent": [
    {
      "name": ["DQN", "DDQN"]
    },
    {
      "name": ["MaxminDQN", "AveragedDQN"],
      "target_networks_num": [2]
    }
  ],
  "device": ["cpu"],
  "hidden_layers": [[64,64]],
  "memory_type": ["Replay"],
  "memory_size": [1e4],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [1e3],
  "epsilon_steps": [1e3],
  "epsilon_start": [1.0],
  "epsilon_end": [0.01],
  "epsilon_decay": [0.999],
  "loss": ["MSELoss"],
  "optimizer": ["RMSprop"],
  "lr": [1e-4],
  "batch_size": [32],
  "display_interval": [50],
  "rolling_score_window": [100],
  "discount": [0.99],
  "seed": [1],
  "gradient_clip": [5],
  "target_network_update_freqency": [200],
  "generate_random_seed": [true]
}