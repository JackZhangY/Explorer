{
  "env": [
    {
      "name": ["LunarLanderContinuous-v2"],
      "max_episode_steps": [-1],
      "input_type": ["feature"]
    }
  ],
  "train_steps": [4e5],
  "test_per_episodes": [10],
  "agent": [{"name": ["REINFORCE", "REINFORCEWithBaseline"]}],
  "optimizer": [
    {
      "name": ["Adam"],
      "actor_kwargs": [{"lr": [1e-3]}]
    }
  ],
  "gradient_clip": [-1],
  "hidden_layers": [[64,64]],
  "hidden_activation": ["Tanh"],
  "display_interval": [50],
  "rolling_score_window": [{"Train": [100], "Test": [10]}],
  "discount": [0.99],
  "seed": [1],
  "device": ["cpu"],
  "show_tb": [true],
  "generate_random_seed": [true]
}